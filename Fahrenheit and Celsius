import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        double f=sc.nextDouble();
        double c=(f-32)*5/9;
        System.out.println(c);
        
            
    }
}
